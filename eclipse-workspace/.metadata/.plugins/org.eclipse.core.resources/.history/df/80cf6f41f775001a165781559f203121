public class DecimalToOctal {
	
	public static int decimalToOctal(String num, int base) {
		//int[] octalNum = new int[100];
		//int i = 0;
		//int octalNum = 0;
		
//		while (num != 0) {
//			octalNum[i] = num%8;
//			num = num/8;
//			i++;
//		}
//		for (int j = i - 1; j >= 0; j--) 
//            System.out.print(octalNum[j]);
		return Integer.parseInt(num, base);
	}

	public static void main(String[] args) {
