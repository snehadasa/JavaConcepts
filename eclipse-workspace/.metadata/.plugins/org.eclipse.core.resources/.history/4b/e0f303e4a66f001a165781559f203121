package learning;

public class ReverseString {

	public static int reverseString(int[] str) {
		for (int i = (str.length - 1); i <= 0; i--) {
			return str[i];
		}
		return -1;
	}
	
	public static void main(String []args) {
		int[] str = {10, 7, 8, 11, 3};
		int res = reverseString(str);
		System.out.println(res);
	}
}
