package learning;

public class DecimalToOctal {
	
	public static void decimalToOctal(int num) {
		//int[] octalNum = new int[100];
		int i = 0;
		int octalNum = 0;
		
		while (num != 0) {
			octalNum = num%8;
			num = num/8;
			i++;
		}
		System.out.println(octalNum);
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = 33;
		decimalToOctal(n);
	}

}
