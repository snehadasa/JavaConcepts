import java.util.List;
import java.util.concurrent.ExecutorService;
import java.util.concurrent.Executors;
import java.util.concurrent.Future;

public class MultiCallableTaskRunner {

	public static void main(String[] args) {
		ExecutorService executorService = Executors.newFixedThreadPool(1);

		List<CallableTask> = List.of(new CallableTask("Hello World"),
				new CallableTask("Sneha"), new CallableTask("Sujith"));
		
		Future<String> welcomeFuture = 
				executorService.submit(new CallableTask("Hello World"));
	}

}
